# Uncomment the next line to define a global platform for your project
platform :ios, '9.0'

target 'TestInk' do
  # Comment the next line if you're not using Swift and don't want to use dynamic frameworks
  use_frameworks!

  # Pods for TestInk
pod 'SnapKit'
pod 'Toucan'
pod 'Firebase/Core'
pod 'Firebase/Auth'
pod 'Firebase/Database'
pod 'Firebase/Storage'

  target 'TestInkTests' do
    inherit! :search_paths
    # Pods for testing
pod 'Firebase/Core'
pod 'Firebase/Auth'
pod 'Firebase/Database'
pod 'Firebase/Storage'

  end

  target 'TestInkUITests' do
    inherit! :search_paths
    # Pods for testing
  end

end
